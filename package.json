{
  "name": "@ajces/observable",
  "version": "0.0.1",
  "description": "mobx inspired observable state",
  "main": "dist/observable.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "peerDependencies": {
    "@ajces/utils": "^1.0.5"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "prettier": "~1.5.3",
    "rollup": "^0.47.6",
    "uglify-js": "^2.7.5"
  },
  "dependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run bundle && npm run minify",
    "bundle": "rollup -i src/index.js -o dist/observable.js -f umd -mn Observable",
    "minify": "uglifyjs dist/observable.js -o dist/observable.js --mangle --compress warnings=false --pure-funcs=Object.defineProperty -p relative --source-map dist/observable.js.map",
    "prepublish": "npm run build",
    "format": "prettier --write 'src/**/*.js'",
    "release": "npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajces/observable.git"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "mobx",
    "observable",
    "state"
  ],
  "author": "Andy Johnson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajces/observable/issues"
  },
  "homepage": "https://github.com/ajces/observable#readme",
  "babel": {
    "presets": "es2015"
  }
}
